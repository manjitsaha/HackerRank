 public static List<Integer> rotateLeft(int d, List<Integer> arr) {
     ArrayList<Integer> list = new ArrayList<>();
      for(int n : arr) {
            list.add(n);
        }
        
        for(int i = 0 ; i < d ; i++) {
            int value = list.get(0);
            list.remove(0);
            list.add(value);
        }
        
        return list;
    }
